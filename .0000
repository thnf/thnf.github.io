function getParams()

{
$url = $_SERVER['HTTP_REFERER'];
$refer_url = parse_url($url);

$params = $refer_url['query'];

$arr = array();

if(!empty($params))

{

$paramsArr = explode('&',$params);

foreach($paramsArr as $k=>$v)

{

$a = explode('=',$v);

$arr[$a[0]] = $a[1];

}

}

return $arr;

}
